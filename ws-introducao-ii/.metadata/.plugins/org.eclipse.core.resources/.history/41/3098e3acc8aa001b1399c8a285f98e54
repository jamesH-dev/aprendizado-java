import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		double num1, num2, num3, mediap;
		for (int i = 0; i < n; i++) {
			num1 = sc.nextDouble();
			num2 = sc.nextDouble();
			num3 = sc.nextDouble();
			for (int j = 0; j < i; j++) {
				mediap = ((num1 * 2) + (num2 * 3) + (num3 * 5) / (num1 + num2 + num3));
				System.out.println(mediap);
			}
		}
		
		sc.close();
	}

}
