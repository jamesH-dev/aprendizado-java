import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite a senha para encerrar o programa");
		int senha = sc.nextInt();
		while (senha != 2002) {
			System.out.println("Senha Inválida, digite novamente!");
			senha = sc.nextInt();
		}
		System.out.println("Senha verificada, programa encerrado!");
		sc.close();
	}

}
