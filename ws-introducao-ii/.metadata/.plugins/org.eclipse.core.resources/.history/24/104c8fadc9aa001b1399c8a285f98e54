import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		
		for (int i=0; i<n; i++) {
			
			int a = sc.nextInt();
			int b = sc.nextInt();

			double div = a / b;
			if (b == 0) {
				System.out.println("Divisão Inválida");
			}
			else {
				System.out.printf("%.2f%n", div);
			}
		}
		
		sc.close();
	}

}
